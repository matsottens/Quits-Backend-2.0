# Quits 2.0 – Environment variables example
# Copy to .env in project root and fill.

NODE_ENV=development
PORT=3000
CLIENT_URL=http://localhost:5173
API_URL=http://localhost:3000

SUPABASE_URL=
# Full service-role (secret) key – keep private, never commit populated value
SUPABASE_SERVICE_ROLE_KEY=
# Public anon key for client-side calls
SUPABASE_ANON_KEY=

GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

VERTEX_PROJECT_ID=quits-2-0
VERTEX_LOCATION=us-central1
GEMINI_API_KEY=

JWT_SECRET=quits-jwt-secret-key-development
JWT_EXPIRES_IN=7d

CORS_ORIGIN=http://localhost:5173 