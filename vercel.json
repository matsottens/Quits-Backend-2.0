{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/auth/callback",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/index.html"
    },
    {
      "src": "/settings",
      "dest": "/index.html"
    },
    {
      "src": "/subscription/:id",
      "dest": "/index.html"
    },
    {
      "src": "/subscriptions",
      "dest": "/index.html"
    },
    {
      "src": "/scan",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://api.quits.cc",
    "VITE_SUPABASE_URL": "@supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@supabase_anon_key",
    "VITE_GOOGLE_CLIENT_ID": "@google_client_id"
  }
} 