{
  "version": 2,
  "buildCommand": null,
  "installCommand": null,
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/vite" },
    { "src": "api/serverless.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/serverless.js" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
} 